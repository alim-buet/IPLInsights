<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="vBox" alignment="CENTER" prefHeight="720.0" prefWidth="1280.0" spacing="40.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AuctionController">
    <children>
        <Label fx:id="title" alignment="CENTER" text="Auction">
            <font>
                <Font name="System Bold" size="21.0" />
            </font>
        </Label>
        <TableView fx:id="playerTable" prefHeight="545.0" prefWidth="1200.0">
            <columns>
                <TableColumn fx:id="nameColumn" prefWidth="200.0" resizable="false" style="-fx-alignment: CENTER;" text="Name" />
                <TableColumn fx:id="positionColumn" prefWidth="150.0" resizable="false" style="-fx-alignment: CENTER;" text="Position" />
                <TableColumn fx:id="ageColumn" resizable="false" style="-fx-alignment: CENTER;" text="Age" />
                <TableColumn fx:id="heightColumn" prefWidth="100.0" resizable="false" style="-fx-alignment: CENTER;" text="Height" />
                <TableColumn fx:id="clubColumn" prefWidth="187.99993896484375" resizable="false" style="-fx-alignment: CENTER;" text="Club" />
                <TableColumn fx:id="numberColumn" prefWidth="112.0" resizable="false" style="-fx-alignment: CENTER" text="Jersey Number" />
                <TableColumn fx:id="weeklySalaryColumn" prefWidth="131.20001220703125" resizable="false" style="-fx-alignment: CENTER;" text="Weekly Salary" />
                <TableColumn fx:id="priceColumn" prefWidth="109.60009765625" resizable="false" style="-fx-alignment: CENTER;" text="Price" />
                <TableColumn fx:id="buyColumn" prefWidth="123.2000732421875" resizable="false" style="-fx-alignment: CENTER;" text="Buy" />
            </columns>
            <VBox.margin>
                <Insets left="20.0" right="20.0" />
            </VBox.margin>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
        <Button mnemonicParsing="false" onAction="#switchToPrimary" prefHeight="50.0" prefWidth="62.0" text="Back" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="30.0" />
    </padding>
</VBox>
