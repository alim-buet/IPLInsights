<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="vBox" alignment="CENTER" prefHeight="720.0" prefWidth="1280.0" spacing="40.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SalaryController">
    <children>
        <Label fx:id="title" alignment="CENTER" text="Total annual salary of each club">
            <font>
                <Font name="System Bold" size="21.0" />
            </font>
        </Label>
        <TableView fx:id="salaryTable" prefHeight="455.0" prefWidth="860.0">
            <columns>
                <TableColumn fx:id="logoColumn" maxWidth="1500.0" prefWidth="40.0" style="-fx-alignment: CENTER;" />
                <TableColumn fx:id="clubColumn" prefWidth="281.0" style="-fx-alignment: CENTER;" text="Club" />
                <TableColumn fx:id="salaryColumn" prefWidth="296.0" style="-fx-alignment: CENTER;" text="Salary" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
            <VBox.margin>
                <Insets left="200.0" right="200.0" />
            </VBox.margin>
        </TableView>
        <Button fx:id="secondaryButton" maxHeight="-Infinity" minHeight="-Infinity" onAction="#switchToPrimary" prefHeight="40.0" prefWidth="69.0" text="Back" />
    </children>
    <padding>
        <Insets bottom="50.0" left="20.0" top="50.0" />
    </padding>
</VBox>
